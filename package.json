{
    "name": "react-typescript-libray-template-engine",
    "version": "1.0.0",
    "description": "Ultimate React TypeScript Libray Template and Packages Genrator",
    "scripts": {
        "setup": "ts-node ./Core/index.ts",
        "init": "ts-node ./Core/index.ts",
        "start": "ts-node ./Core/index.ts",
        "add:package": "ts-node ./Core/index.ts #add %package",
        "add:Test": "ts-node ./Core/index.ts #add %Test",
        "remove:package": "ts-node ./Core/index.ts #remove %package",
        "remove:Test": "ts-node ./Core/index.ts #remove %Test",
        "link:package": "ts-node ./Core/index.ts #link %package",
        "link:Test": "ts-node ./Core/index.ts #link %Test",
        "publish:package": "ts-node ./Core/index.ts #publish %package",
        "publish:Test": "ts-node ./Core/index.ts #publish %Test",
        "reset:package": "ts-node ./Core/index.ts #reset %package",
        "reset:Test": "ts-node ./Core/index.ts #reset %Test",
        "build:package": "ts-node ./Core/index.ts #build %package",
        "build:Test": "ts-node ./Core/index.ts #build %Test"
    },
    "keywords": [
        "react",
        "react-typescript",
        "typescript",
        "react-template",
        "react-typescript-library",
        "react-typescript-library-template",
        "template"
    ],
    "author": "@TeamSM/MeetBhingradiya",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/node": "^18.11.15",
        "@types/prompt": "^1.1.4"
    },
    "dependencies": {
        "dot-env": "^0.0.1",
        "prompt": "^1.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "winston": "^3.8.2"
    }
}
